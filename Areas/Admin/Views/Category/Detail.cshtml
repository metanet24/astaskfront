@model CategoryDetailVM
@{
    ViewData["Title"] = "Detail";
}

<div class="container mt-5">
    <form class="forms-sample" method="post" asp-action="Create">
        <div class="form-group">
            <label>Category</label>
            <input asp-for="Name" type="text" disabled class="form-control">
        </div>
        <img src="~/assets/images/@Model.Image" width="100" height="100" />
        <a asp-action="Index" class="btn btn-primary text-white">back</a>
    </form>
</div>


